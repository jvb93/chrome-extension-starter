{
  "description": "A lightweight theme using a pastel interpretation of the google colors",
  "manifest_version": 2,
  "version": "1.0",
  "name": "Google Colors Pastel",
  "theme": {
    "colors" : {
      "frame" : [105, 105, 105],
      "toolbar" : [150, 200, 255],
      "ntp_text" : [105, 105, 105],
      "ntp_link" : [105, 105, 105],
      "ntp_section" : [105, 105, 105],
      "ntp_background": [245, 245, 245],
      "button_background" : [128, 128, 128]
    },
    "images": {
    "theme_frame": "images/theme_frame.png",
    "theme_tab_background": "images/theme_tab_background.png" ,
    "theme_toolbar": "images/theme_toolbar.png" 
    },
    "properties": {
      "ntp_background_alignment": "bottom",
      "ntp_background_repeat": "no-repeat"
    },
    "tints" : {
      "buttons" : [-1.0, -1.0, -1.0]
    }
  }
}